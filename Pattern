#include<stdio.h>
void main(){
    for(int i=1;i<=5;i++){
        for(int j=1;j<=9;j++){
            if(j==6-i | j==4+i ){
                if(j==6-i | j==4+i){
                printf("*");
                }
                else{
                    printf(" ");
                }
            }
            else if(i==5){
                if(j>=6-i | j<=4+i){
                    printf("*");
                }
                else{
                    
                }
            }
            
            // (6-I & 4+i) or(6-i & 4+i)
            // & j>=6-i & j<=4+i
            // else if(j>=10/2+i){
            //     printf("*");
                
